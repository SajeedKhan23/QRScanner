      },
      "googleServicesFile": "./google-services.json",
      "package": "com.sajeed03.QRApp1"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6575ca31-6f28-4e04-bacc-6ed1981725e1"
      }
    }
